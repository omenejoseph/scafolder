{
    "name": "omenejoseph/scafolder",
    "description": ":scafolding laravel files",
    "license": "MIT",
    "authors": [
        {
            "name": "Omene Joseph",
            "email": "runoomene01@gmail.com",
            "homepage": "https://omenejoseph.com.ng"
        }
    ],
    "homepage": "https://github.com/omenejoseph/scafolder",
    "keywords": ["Laravel", "Scafolder"],
    "require": {
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "OmeneJoseph\\Scafolder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OmeneJoseph\\Scafolder\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OmeneJoseph\\Scafolder\\ScafolderServiceProvider"
            ],
            "aliases": {
                "Scafolder": "OmeneJoseph\\Scafolder\\Facades\\Scafolder"
            }
        }
    }
}
